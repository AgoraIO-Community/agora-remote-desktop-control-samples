{
  "name": "rdc-client-angular",
  "version": "2.0.0",
  "main": "build/electron/main.js",
  "scripts": {
    "update-electron": "node scripts/update-electron",
    "ng": "ng",
    "start": "concurrently \"ng serve\" \"tsc -p electron && cross-env NODE_ENV=development electron .\"",
    "build": "ng build --configuration=production && tsc -p electron && electron-builder",
    "watch": "ng build --watch --configuration development",
    "test": "ng test"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^14.0.4",
    "@angular/common": "^14.0.4",
    "@angular/compiler": "^14.0.4",
    "@angular/core": "^14.0.4",
    "@angular/forms": "^14.0.4",
    "@angular/platform-browser": "^14.0.4",
    "@angular/platform-browser-dynamic": "^14.0.4",
    "@angular/router": "^14.0.4",
    "agora-electron-sdk": "3.8.202-build.619",
    "agora-rdc-core": "^1.0.2",
    "agora-rdc-electron": "^2.0.1-alpha.2",
    "agora-rdc-webrtc-electron": "^1.0.2",
    "agora-rtc-sdk-ng": "^4.8.1",
    "ng-zorro-antd": "^13.3.2",
    "rxjs": "~7.4.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-builders/custom-webpack": "^14.0.0",
    "@angular-devkit/build-angular": "^14.0.4",
    "@angular/cli": "^14.0.4",
    "@angular/compiler-cli": "^14.0.4",
    "@types/jasmine": "~3.10.0",
    "@types/node": "^12.11.1",
    "electron": "12.0.0",
    "electron-builder": "^23.2.0",
    "jasmine-core": "~3.10.0",
    "karma": "~6.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.1.0",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "~1.7.0",
    "typescript": "~4.7.4"
  },
  "build": {
    "extends": null,
    "asar": true,
    "asarUnpack": [
      "node_modules/agora-rdc-core/**/*",
      "node_modules/agora-electron-sdk/**/*"
    ],
    "afterPack": "./after-pack.js",
    "files": [
      "build/**/*",
      "node_modules/**/*",
      "!**/node_modules/**/agora-rdc-core/sdk/**/*",
      "!**/node_modules/**/{CHANGELOG.md,README.md,README,readme.md,README.zh.md}",
      "!**/node_modules/**/{nodemon.json,rollup.config.js,tsconfig.json,typedoc.json,rc_message.proto}",
      "!**/node_modules/**/*.{ts,h,cc,cpp,gyp,map,hpp,mm,proto,cmd,tar.gz,a,la,inc,pc,dSYM}",
      "!**/node_modules/*/{test,__tests__,tests,powered-test,example,examples}",
      "!**/node_modules/.bin",
      "!**/*.{iml,o,hprof,orig,pyc,pyo,rbc,swp,csproj,sln,xproj}",
      "!.editorconfig",
      "!**/{.DS_Store,.git,.hg,.svn,CVS,RCS,SCCS,.gitignore,.gitattributes}",
      "!**/{__pycache__,thumbs.db,.flowconfig,.idea,.vs,.nyc_output}",
      "!**/{appveyor.yml,.travis.yml,circle.yml}",
      "!**/{npm-debug.log,yarn.lock,.yarn-integrity,.yarn-metadata.json}"
    ],
    "artifactName": "${productName}-v${version}.build${env.BUILD_NUMBER}-${os}-${arch}.${ext}",
    "productName": "RDCClient-Angular",
    "appId": "io.agora.rdc-client-angular",
    "mac": {
      "target": "dmg",
      "entitlements": "entitlements.mac.plist",
      "hardenedRuntime": true,
      "signIgnore": [
        "/\\.framework$/"
      ]
    },
    "win": {
      "target": "nsis",
      "requestedExecutionLevel": "requireAdministrator"
    },
    "nsis": {
      "license": "./eula.txt"
    }
  },
  "agora_electron": {
    "electron_version": "12.0.0",
    "prebuilt": true,
    "arch": "x64"
  }
}