{
  "name": "primary",
  "version": "0.1.7",
  "private": true,
  "license": "MIT",
  "main": "build/electron/main.js",
  "homepage": "./",
  "author": {
    "name": "Agora",
    "email": "agoralegal@agora.io",
    "url": "https://www.agora.io/"
  },
  "scripts": {
    "start": "concurrently \"cross-env BROWSER=none react-app-rewired start\" \"tsc -p electron && cross-env NODE_ENV=development electron .\"",
    "build:testing": "yarn clean && cross-env TARGET=testing react-app-rewired build && tsc -p electron && electron-builder",
    "build:prod": "yarn clean && cross-env TARGET=production react-app-rewired build && tsc -p electron && electron-builder",
    "clean": "rimraf ./build ./dist"
  },
  "dependencies": {
    "agora-electron-sdk": "3.4.7-rc.1-build.817",
    "agora-rdc-electron": "^1.0.0-alpha.1",
    "agora-rdc-webrtc-electron": "^1.0.0-alpha.1",
    "agora-rtc-sdk-ng": "^4.6.3",
    "antd": "^4.16.7",
    "axios": "^0.21.1",
    "electron-devtools-installer": "^3.2.0",
    "electron-log": "^4.3.5",
    "electron-updater": "^4.3.9",
    "ora": "^5.4.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^5.2.0",
    "react-use": "^17.2.4",
    "styled-components": "^5.3.0",
    "web-vitals": "^1.0.1"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "@types/jest": "^26.0.15",
    "@types/node": "^12.0.12",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/react-router-dom": "^5.1.8",
    "concurrently": "^6.2.0",
    "cross-env": "^7.0.3",
    "customize-cra": "^1.0.0",
    "electron": "7.1.2",
    "electron-builder": "^22.11.11",
    "react-app-rewired": "^2.1.8",
    "react-scripts": "4.0.3",
    "rimraf": "^3.0.2",
    "typescript": "^4.1.2"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "build": {
    "extends": null,
    "asar": true,
    "afterPack": "./after-pack.js",
    "files": [
      "build/**/*",
      "node_modules/**/*",
      "!**/node_modules/**/{CHANGELOG.md,README.md,README,readme.md,README.zh.md}",
      "!**/node_modules/**/{nodemon.json,rollup.config.js,tsconfig.json,typedoc.json,rc_message.proto}",
      "!**/node_modules/**/*.{ts,h,cc,cpp,gyp,map,hpp,mm,proto,cmd,tar.gz,a,la,inc,pc,dSYM}",
      "!**/node_modules/*/{test,__tests__,tests,powered-test,example,examples}",
      "!**/node_modules/.bin",
      "!**/*.{iml,o,hprof,orig,pyc,pyo,rbc,swp,csproj,sln,xproj}",
      "!.editorconfig",
      "!**/{.DS_Store,.git,.hg,.svn,CVS,RCS,SCCS,.gitignore,.gitattributes}",
      "!**/{__pycache__,thumbs.db,.flowconfig,.idea,.vs,.nyc_output}",
      "!**/{appveyor.yml,.travis.yml,circle.yml}",
      "!**/{npm-debug.log,yarn.lock,.yarn-integrity,.yarn-metadata.json}"
    ],
    "artifactName": "${productName}-v${version}.build${env.BUILD_NUMBER}-${os}-${arch}.${ext}",
    "productName": "RDCPrimary",
    "appId": "io.agora.primary",
    "mac": {
      "target": "dmg",
      "category": "io.agora.metis.category.type",
      "entitlements": "entitlements.mac.plist",
      "hardenedRuntime": true
    },
    "win": {
      "target": "nsis"
    },
    "nsis": {
      "license": "./eula.txt"
    }
  }
}
